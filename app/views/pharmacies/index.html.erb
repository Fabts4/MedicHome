<h1>Pharmacies#index</h1>
<p>Find me in app/views/pharmacies/index.html.erb</p>

<h1>List of Pharmacies</h1>
<ul>
  <% @pharmacies.each do |pharmacy| %>
    <li><%= link_to pharmacy.name, prescription_pharmacy_path(@prescription, pharmacy) %></li>
    <li><%= pharmacy.address %></li>
  <% end %>
</ul>
